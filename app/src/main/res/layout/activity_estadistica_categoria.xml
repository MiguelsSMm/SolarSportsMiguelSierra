<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Estadistica_Categoria_Activity">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="889dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="54dp"
            android:layout_marginTop="20sp"
            android:backgroundTint="#FFFFFF"
            android:contentDescription="botonVolver"
            app:srcCompat="?attr/actionModeCloseDrawable" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="344dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="40sp"
            android:layout_marginBottom="20dp"
            android:fontFamily="@font/urbanist_bold"
            android:text="Categoria Cancha"
            android:textColor="#1E232C"
            android:textSize="34sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="25sp"
            android:fontFamily="@font/urbanist_semibold"
            android:text="Estadistica terraza solar"
            android:textColor="#54595E"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textViewPromedio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="10sp"
            android:fontFamily="@font/urbanist_medium"
            android:text="Promedio total de energia producida"
            android:textColor="#4F4F4F"
            android:textSize="16sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:layout_marginBottom="40sp"
            android:background="@drawable/edittext_background"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="285dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:fontFamily="@font/urbanist_medium"
                android:paddingLeft="20dp"
                android:text="Promedio total"
                android:textColor="#8391A1"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:fontFamily="@font/urbanist_medium"
                android:text="Vatios (W)"
                android:textColor="#8391A1"
                android:textSize="18sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginBottom="10sp"
            android:fontFamily="@font/urbanist_medium"
            android:text="Cantidad de energia producida"
            android:textColor="#4F4F4F"
            android:textSize="16sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:layout_marginBottom="20sp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="195dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="172dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/urbanist_medium"
                    android:text="Minimo"
                    android:textColor="#4F4F4F" />

                <LinearLayout
                    android:layout_width="173dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/edittext_background"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="63dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:hint="Min"
                        android:paddingLeft="5dp" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="32dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="Vatios (W)"
                        android:textColor="#8391A1" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="257dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/urbanist_medium"
                    android:text="Maximo"
                    android:textColor="#4F4F4F" />

                <LinearLayout
                    android:layout_width="173dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/edittext_background"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="94dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:hint="Max"
                        android:paddingLeft="5dp" />

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right|center_vertical"
                        android:layout_weight="1"
                        android:text="Vatios (W)"
                        android:textColor="#8391A1" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>